{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "imports": {
    "std/": "https://deno.land/std@0.190.0/",
    "jikji/": "https://raw.githubusercontent.com/dahlia/jikji/c53a5f64f318aa973619c48b07d3727de6d64a47/"
  },
  "lint": {
    "exclude": ["public_html", "static"]
  },
  "fmt": {
    "include": ["main.ts", "tmpdir.ts"]
  },
  "lock": "lock.json",
  "tasks": {
    "build": "deno run --allow-read --allow-write=.bin,public_html,\"$(deno task tmpdir)\" --allow-env=SEONBI_API,TMPDIR,TMP,TEMP --allow-run --allow-net=deno.land,cdn.skypack.dev,github.com,objects.githubusercontent.com,127.0.0.1 main.ts",
    "cache": "deno cache --lock-write main.ts",
    "serve": "deno task build --watch --serve",
    "tmpdir": "deno run --allow-env=TMPDIR,TMP,TEMP,SYSTEMROOT tmpdir.ts"
  }
}
